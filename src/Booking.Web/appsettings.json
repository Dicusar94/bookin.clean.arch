{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=booking;Username=myuser;Password=mypassword"
  },
  "TickerQBasicAuth": {
    "Username": "admin",
    "Password": "admin"
  },
  "OpenTelemetrySettings": {
    "Enabled": true,
    "TracesEndpoint": "http://localhost:4317",
    "Attributes": [
      {
        "Key": "OS",
        "Value": "MacOs"
      }
    ]
  },
  "AllowedHosts": "*",
  "feature_management": {
    "feature_flags": [
      {
        "id": "default-override",
        "enabled": false 
      },
      {
        "id": "logging-middleware",
        "enabled": true
      },
      {
        "id": "TestService",
        "enabled": false,
        "allocation": {
          "default_when_enabled": "TestOne",
          "default_when_disabled": "TestTwo"
        },
        "variants": [
          { "Name": "TestOne"},
          { "Name": "TestTwo"}
        ]
      }
    ]
  }
}
