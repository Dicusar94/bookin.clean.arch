{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "feature_management": {
    "feature_flags": [
      {
        "id": "default-override",
        "enabled": false 
      },
      {
        "id": "logging-middleware",
        "enabled": false 
      },
      {
        "id": "TestService",
        "enabled": false,
        "allocation": {
          "default_when_enabled": "TestOne",
          "default_when_disabled": "TestTwo"
        },
        "variants": [
          { "Name": "TestOne"},
          { "Name": "TestTwo"}
        ]
      }
    ]
  }
}
